{
  "id": "token-reporter",
  "name": "Token Reporter",
  "description": "每日Token消耗报告插件 - 自动统计并报告AI使用量",
  "version": "1.0.0",
  "configSchema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "reportTime": {
        "type": "string",
        "default": "08:00",
        "description": "每日报告时间 (HH:mm)"
      },
      "channels": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "default": [],
        "description": "报告发送频道ID列表"
      },
      "providers": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "default": ["minimax-portal", "deepseek", "moonshot"],
        "description": "需要统计的AI提供商"
      }
    }
  },
  "skills": []
}
